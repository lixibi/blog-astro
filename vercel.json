{
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/admin/"
    }
  ],
  "functions": {
    "src/pages/**/*.astro": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/admin/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}